{
  "id": "MMObit 18.5",
  "name": "MMObit-FARM",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "ID",
      "minVersion": "18.5"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "command",
      "id": "showAlert",
      "label": "Show alert"
    },
    {
      "type": "panel",
      "id": "showPanel",
      "minimumSize": {
        "width": 750,
        "height": 700
      },
      "maximumSize": {
        "width": 750,
        "height": 700
      },
   
      "label": {
        "default": "Show panel"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ],
  "requiredPermissions": {

    "localFileSystem": "fullAccess",
      "network": {
        "domains": ["https://cdnjs.cloudflare.com/ajax/libs/jqtree/1.7.3/tree.jquery.js","https://cdnjs.cloudflare.com/ajax/libs/jqtree/1.4.8/jqtree.css","https://code.jquery.com/jquery-3.6.0.min.js"]
      }
    },
    "dependencies": {
      "uxp": "^7.1.1"
    }
    
  }
